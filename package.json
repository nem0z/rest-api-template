{
  "name": "rest-api-template",
  "version": "1.0.0",
  "description": "Express and typescript template for REST API",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --watch src -e js,ts,json --exec \"ts-node src/index.ts\"",
    "build": "tsup src/index.ts --format cjs --clean",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nem0z/rest-api-template.git"
  },
  "keywords": [
    "express",
    "typescript",
    "rest",
    "api",
    "template",
    "auth"
  ],
  "author": "nem0z",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nem0z/rest-api-template/issues"
  },
  "homepage": "https://github.com/nem0z/rest-api-template#readme",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.17",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.5.2",
    "sqlite3": "^5.1.4"
  }
}
